    private void jbtnAgregarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtnAgregarActionPerformed
        // TODO add your handling code here:
        if(jtxDepTemporal.getText().equals(""))
        {
        }
        else
        {
            model.addElement(jtxDepTemporal.getText());
            jlstDependencia.setSelectedIndex(-1);
            jtxDepTemporal.setText("");
        }
    }//GEN-LAST:event_jbtnAgregarActionPerformed

    private void jbtnEliminarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtnEliminarActionPerformed
        // TODO add your handling code here:
        if(jlstDependencia.getSelectedIndex() != -1)
        {
            model.remove(jlstDependencia.getSelectedIndex());
            jlstDependencia.setSelectedIndex(-1);
        }
    }//GEN-LAST:event_jbtnEliminarActionPerformed

    private void jtxDepTemporalKeyReleased(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_jtxDepTemporalKeyReleased
        // TODO add your handling code here:
        if(evt.getKeyCode() == KeyEvent.VK_ENTER)
        {
            if(jtxDepTemporal.getText().equals(""))
            {
            }
            else
            {
                model.addElement(jtxDepTemporal.getText());
                jlstDependencia.setSelectedIndex(-1);
                jtxDepTemporal.setText("");
            }
        }
    }//GEN-LAST:event_jtxDepTemporalKeyReleased

    private void jbtnAceptarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtnAceptarActionPerformed
        // TODO add your handling code here:
        if(!this.editar)
        {
            ArrayList<String> dep = new ArrayList<String>();
            for(int i = 0; i < model.capacity(); i++)
            {
                try
                {
                    if(!dep.contains((String)model.get(i)))
                        dep.add((String)model.get(i));
                    System.out.println((String)model.get(i));
                }
                catch(ArrayIndexOutOfBoundsException ex)
                {
                }
            }
            DefaultMutableTreeNode nodo=null;
            RFuncional req=new RFuncional();
            req.setNombre(this.jtxNombre.getText());
            if (this.jcbFuncional.isSelected())
                req.setTipo(true);
            else if(this.jcbNoFuncional.isSelected())
                req.setTipo(false);
            req.setVersion(this.jtxVersion.getText());
            req.setDescripcion(this.jtxaDescripcion.getText());
            req.setImportancia((String)this.jcmbImportancia.getSelectedItem());
            req.setUrgencia((String)this.jcmbUrgencia.getSelectedItem());
            req.setEstado((String)this.jcmbEstabilidad.getSelectedItem());
            req.setEstabilidad((String)this.jcmbEstabilidad.getSelectedItem());
            req.setComentario(this.jtxaComentarios.getText());
            req.setDependencia(dep);
            req.addObserver(this.padre);
            this.padre.getDoc().Crear_Req(req);
            nodo=new DefaultMutableTreeNode(req);
            this.nodo_insercion.add(nodo);
            this.padre.añadir_cosa(nodo);
            req.notificar_observer();
        }
        else
        {
            RFuncional req=(RFuncional) this.nodo_insercion.getUserObject();
            ArrayList<String> dep = new ArrayList<String>();
            for(int i = 0; i < model.capacity(); i++)
            {
                try
                {
                    if(!dep.contains((String)model.get(i)))
                        dep.add((String)model.get(i));
                    System.out.println((String)model.get(i));
                }
                catch(ArrayIndexOutOfBoundsException ex)
                {
                }
            }
            req.setNombre(this.jtxNombre.getText());
            if (this.jcbFuncional.isSelected())
                req.setTipo(true);
            else if(this.jcbNoFuncional.isSelected())
                req.setTipo(false);
            req.setVersion(this.jtxVersion.getText());
            req.setDescripcion(this.jtxaDescripcion.getText());
            req.setImportancia((String)this.jcmbImportancia.getSelectedItem());
            req.setUrgencia((String)this.jcmbUrgencia.getSelectedItem());
            req.setEstado((String)this.jcmbEstabilidad.getSelectedItem());
            req.setEstabilidad((String)this.jcmbEstabilidad.getSelectedItem());
            req.setComentario(this.jtxaComentarios.getText());
            req.setDependencia(dep);
            req.notificar_observer();
        }
        this.padre.setEnabled(true);
        this.dispose();
    }//GEN-LAST:event_jbtnAceptarActionPerformed

