    private void jcbFuncionalActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jcbFuncionalActionPerformed
        // TODO add your handling code here:
        jcbFuncional.setSelected(true);
        jcbNoFuncional.setSelected(false);

        if((jtxNombre.getText().equals("")) || (!jcbFuncional.isSelected() &&
                !jcbNoFuncional.isSelected()))
        {
            jbtnAceptar.setEnabled(false);
        }
        else
            jbtnAceptar.setEnabled(true);
    }//GEN-LAST:event_jcbFuncionalActionPerformed

    private void jcbNoFuncionalActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jcbNoFuncionalActionPerformed
        // TODO add your handling code here:
        jcbFuncional.setSelected(false);
        jcbNoFuncional.setSelected(true);

        if((jtxNombre.getText().equals("")) || (!jcbFuncional.isSelected() &&
                !jcbNoFuncional.isSelected()))
        {
            jbtnAceptar.setEnabled(false);
        }
        else
            jbtnAceptar.setEnabled(true);
    }//GEN-LAST:event_jcbNoFuncionalActionPerformed

    private void formWindowClosing(java.awt.event.WindowEvent evt) {//GEN-FIRST:event_formWindowClosing
        // TODO add your handling code here:
        if(padre != null)
            padre.setEnabled(true);
        this.dispose();
    }//GEN-LAST:event_formWindowClosing

    private void jbtnCancelarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtnCancelarActionPerformed
        // TODO add your handling code here:
        if(padre != null)
            padre.setEnabled(true);
