        this.nodo_insercion=nodo;
        this.editar=n_editar;
        if(n_editar)this.inicializar_edicion();
    }

    
    private void inicializar_edicion()
    {
        RFuncional rf=(RFuncional) this.nodo_insercion.getUserObject();
        this.jtxNombre.setText(rf.getNombre());
        if(rf.getTipo())
        {
            this.jcbFuncional.setSelected(true);
            this.jcbNoFuncional.setSelected(false);
        }
        else
        {
            this.jcbFuncional.setSelected(false);
            this.jcbNoFuncional.setSelected(true);
        }
        this.jtxVersion.setText(rf.getVersion());
        this.jtxaDescripcion.setText(rf.getDescripcion());
        ArrayList<String> p=rf.getDependencia();
        for(String g:p)
            this.model.addElement(g);
        this.jcmbImportancia.setSelectedItem(rf.getImportancia());
        this.jcmbEstabilidad.setSelectedItem(rf.getEstabilidad());
        this.jcmbEstado.setSelectedItem(rf.getEstado());
        this.jcmbUrgencia.setSelectedItem(rf.getUrgencia());
        this.jtxaComentarios.setText(rf.getComentario());
    }
    
    
