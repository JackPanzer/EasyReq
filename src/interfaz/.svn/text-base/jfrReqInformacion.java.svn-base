    public jfrReqInformacion(Interfaz padre,DefaultMutableTreeNode nodo,boolean n_editar) {
        initComponents();
        this.padre = padre;
        jbtnAceptar.setEnabled(false);
        model = new DefaultListModel();
        jlstDependencia.setModel(model);
        this.nodo_insercion=nodo;
        this.editar=n_editar;
        if(n_editar)this.inicializar_edicion();
    }

    
    private void inicializar_edicion()
    {
        RInformacion ri=(RInformacion) this.nodo_insercion.getUserObject();
        this.jtxNombre.setText(ri.getNombre());
        if(ri.isTipo())
        {
            this.jcbInfo.setSelected(true);
            this.jcbArquitectura.setSelected(false);
        }
        else
        {
            this.jcbInfo.setSelected(false);
            this.jcbArquitectura.setSelected(true);
        }
        this.jtxVersion.setText(ri.getVersion());
        this.jtxaDescripcion.setText(ri.getDescripcion());
        for(String aux:ri.getDependencia())
            this.model.addElement(aux);
        this.jtxTiempoVidaU.setText(ri.getTv_unidad());
        this.jtxTiempoOcupadoC.setText(ri.getTv_cantidad());
        this.jtxTiempoOcupadoU.setText(ri.getTocu_unidad());
        this.jtxTiempoOcupadoC.setText(ri.getTocu_cantidad());
        this.jcmbImportancia.setSelectedItem(ri.getImportancia());
        this.jcmbEstado.setSelectedItem(ri.getEstado());
        this.jcmbEstabilidad.setSelectedItem(ri.getEstabilidad());
        this.jcmbUrgencia.setSelectedItem(ri.getUrgencia());
        this.jtxaComentarios.setText(ri.getComentario());
    }
    
