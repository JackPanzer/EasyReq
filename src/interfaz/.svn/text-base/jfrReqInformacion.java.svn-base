    private void jbtnAceptarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jbtnAceptarActionPerformed
        // TODO add your handling code here:
        if(!this.editar)
        {
            ArrayList<String> dep = new ArrayList<String>();
            for(int i = 0; i < model.capacity(); i++)
            {
                try
                {
                    if(!dep.contains((String)model.get(i)))
                        dep.add((String)model.get(i));
                    System.out.println((String)model.get(i));
                }
                catch(ArrayIndexOutOfBoundsException ex)
                {
                }
            }
            DefaultMutableTreeNode nodo=null;
            RInformacion ri=new RInformacion();
            ri.setNombre(this.jtxNombre.getText());
            if(this.jcbInfo.isSelected())
                ri.setTipo(true);
            else if(this.jcbArquitectura.isSelected())
                ri.setTipo(false);
            ri.setVersion(this.jtxVersion.getText());
            ri.setDescripcion(this.jtxaDescripcion.getText());
            ri.setDependencia(dep);
            ri.setImportancia((String)this.jcmbImportancia.getSelectedItem());
            ri.setUrgencia((String)this.jcmbUrgencia.getSelectedItem());
            ri.setEstado((String)this.jcmbEstado.getSelectedItem());
            ri.setEstabilidad((String)this.jcmbEstabilidad.getSelectedItem());
            ri.setComentario(this.jtxaComentarios.getText());
            ri.setTv_cantidad(this.jtxTiempoVidaC.getText());
            ri.setTv_unidad(this.jtxTiempoVidaU.getText());
            ri.setTocu_cantidad(this.jtxTiempoOcupadoC.getText());
            ri.setTocu_unidad(this.jtxTiempoOcupadoU.getText());
            ri.addObserver(this.padre);
            this.padre.getDoc().Crear_Req(ri);
            nodo=new DefaultMutableTreeNode(ri);
            this.nodo_insercion.add(nodo);
            this.padre.añadir_cosa(nodo);
            ri.notificar_observer();
        }
        else
        {
            RInformacion ri=(RInformacion) this.nodo_insercion.getUserObject();
            ArrayList<String> dep = new ArrayList<String>();
            for(int i = 0; i < model.capacity(); i++)
            {
                try
                {
                    if(!dep.contains((String)model.get(i)))
                        dep.add((String)model.get(i));
                    System.out.println((String)model.get(i));
                }
                catch(ArrayIndexOutOfBoundsException ex)
                {
                }
                ri.setNombre(this.jtxNombre.getText());
            if(this.jcbInfo.isSelected())
                ri.setTipo(true);
            else if(this.jcbArquitectura.isSelected())
                ri.setTipo(false);
            ri.setVersion(this.jtxVersion.getText());
            ri.setDescripcion(this.jtxaDescripcion.getText());
            ri.setDependencia(dep);
            ri.setImportancia((String)this.jcmbImportancia.getSelectedItem());
            ri.setUrgencia((String)this.jcmbUrgencia.getSelectedItem());
            ri.setEstado((String)this.jcmbEstado.getSelectedItem());
            ri.setEstabilidad((String)this.jcmbEstabilidad.getSelectedItem());
            ri.setComentario(this.jtxaComentarios.getText());
            ri.setTv_cantidad(this.jtxTiempoVidaC.getText());
            ri.setTv_unidad(this.jtxTiempoVidaU.getText());
            ri.setTocu_cantidad(this.jtxTiempoOcupadoC.getText());
            ri.setTocu_unidad(this.jtxTiempoOcupadoU.getText());
            ri.notificar_observer();
            }
            
        }
        this.padre.setEnabled(true);
        this.dispose();
    }//GEN-LAST:event_jbtnAceptarActionPerformed

